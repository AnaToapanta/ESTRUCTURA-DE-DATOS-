class Vehiculo
{
    public string Placa;
    public string Marca;
    public string Modelo;
    public int A単o;
    public double Precio;

    public Vehiculo(string placa, string marca, string modelo, int a単o, double precio)
    {
        Placa = placa;
        Marca = marca;
        Modelo = modelo;
        A単o = a単o;
        Precio = precio;
    }
}